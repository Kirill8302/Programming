#include <iostream>
#include <fstream>
#include <string>
using namespace std;
int main(int argc, char* argv[])
{
    setlocale(LC_ALL, "Russian");
    ifstream fileF1("F1.txt");
    ofstream fileF2("F2.txt");
    if (!fileF1.is_open() || !fileF2.is_open()) return 1;
    string str;
    while (getline(fileF1, str))
        if (*str.begin() == 'A')
            fileF2 << str << endl;
    return 0;
}
