#include <cstdarg>
using namespace std;
// Функция mult с переменным числом параметров, которая находит произведение чисел типа float
float mult(int k, ...)
{
	float result = 1;
	va_list args;
	va_start(args, k);
	for (int i = 0; i < k; ++i) {
		result *= va_arg(args, double);
	}
	va_end(args);
	return result;
}
// Функцию main, которая обращается к функции mult
int main()
{
	cout << mult(3, 1.1f, 2.2f, 3.3f) << '\n';
	cout << mult(7, 1.1f, 2.2f, 3.3f, 4.4f, 5.5f, 6.6f, 7.7f) << '\n';
	cout << mult(11, 1.1f, 2.2f, 3.3f, 4.4f, 5.5f, 6.6f, 7.7f, 8.8f, 9.9f, 10.1f, 11.1f) << '\n';
}
